<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>MyBank</display-name>
  <welcome-file-list>
    <welcome-file>Intro.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>bank</servlet-name>
    <servlet-class>ServletBank</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bank</servlet-name>
    <url-pattern>/BankingServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>string</servlet-name>
    <servlet-class>StringServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>string</servlet-name>
    <url-pattern>/newServlet</url-pattern>
  </servlet-mapping>
  
</web-app>